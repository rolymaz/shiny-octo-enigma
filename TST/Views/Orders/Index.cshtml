@model TST.Models.OrderModel



<h2>Order Info</h2>

<div>
   
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Order.Id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Order.Id)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Order.OrderQueueId)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Order.OrderQueueId)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Order.OrderQueueChangeDate)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Order.OrderQueueChangeDate)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Order.Notes)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Order.Notes)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Order.CreateDate))
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Order.CreateDate)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Order.TeamId)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Order.TeamId)
        </dd>

    </dl>
</div>


<hr />
<h2>Order Logs</h2>




<table class="table">
    <tr>
        <th>
            OrderId
        </th>
        <th>
            Source
        </th>
        <th>
            Destination
        </th>
        <th>
           Result
        </th>
        <th>
           ChangeDate
        </th>
        <th>
            ChangeBy
        </th>
        <th>
            EnterDate
        </th>
        <th>
            ExitDate
        </th>
        <th>
            MinutesInQueue
        </th>
        <th>
            ReasonId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.OrderLogs) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OrderId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Source)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Destination)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Result)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChangeDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChangeBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnterDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExitDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinutesInQueue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReasonId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
